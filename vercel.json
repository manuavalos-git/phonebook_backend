{
    "version": 2,
    "routes": [
      {
        "src": "/api/.*",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/build"
      }
    ],
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build",
        "use": "@vercel/static"
      }
    ]
  }
